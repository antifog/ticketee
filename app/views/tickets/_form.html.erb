<%= form_for [@project, @ticket] do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :title %><br>
      </programlisting>
      </informalexample>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :state_id %>
    <%= f.select :state_id , @states.map { |s| [s.name, s.id] } , :selected => @ticket.state_id %>
  </p>
  <%= f.submit %>
<% end %>
